{
    "name": "gupalo/monolog-dbal-logger",
    "description": "Monolog Handler for DBAL",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^2.10|^3.2",
        "monolog/monolog": "^3",
        "symfony/console": "^5.4|^6.0",
        "symfony/event-dispatcher": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\Tests\\": "tests/"
        }
    }
}
