{
    "name": "gupalo/monolog-dbal-logger",
    "description": "Monolog Handler for DBAL",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^3.8|^4.0",
        "monolog/monolog": "^3.5",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/event-dispatcher": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "doctrine/orm": "^3.3",
        "easycorp/easyadmin-bundle": "^4.18",
        "gupalo/symfony-form-transformers": "^1.3",
        "marcin-jozwikowski/easy-admin-pretty-urls": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\Tests\\": "tests/"
        }
    }
}
