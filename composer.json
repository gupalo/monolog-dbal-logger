{
    "name": "gupalo/monolog-dbal-logger",
    "description": "Monolog Handler for DBAL",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^2.10",
        "monolog/monolog": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\MonologDbalLogger\\Tests\\": "tests/"
        }
    }
}
